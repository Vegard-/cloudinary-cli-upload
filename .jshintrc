  "bitwise": true,
  "curly": false,
  "eqeqeq": true,
  "forin": true,
  "funcscope": false,
  "freeze": true,
  "iterator": false,
  "lastsemic": false,
  "latedef": true,
  "noarg": true,
  "notypeof": false,
  "nonbsp": true,
  "nonew": true,
  "shadow": false,
  "strict": false,
  "undef": true,
  "unused": true,
  /* relaxing */
