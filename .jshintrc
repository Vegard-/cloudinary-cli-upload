  "boss": true,
  "debug": false,
  "eqnull": true,
  "esnext": false,
  "evil": false,
  "expr": true,
  "gcl": false,
  "globalstrict": false,
  "loopfunc": false,
  "moz": false,
  "noyield": false,
  "plusplus": false,
  "proto": false,
  "scripturl": false,
  "supernew": false,
  /* environment */
